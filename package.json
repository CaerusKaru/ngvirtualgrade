{
  "name": "ngvirtualgrade",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "start:hmr": "ng serve --hmr --e=hmr --proxy-config proxy.conf.json",
    "build:dev": "ng build --prod --e=dev --build-optimizer && ng build --prod --e=dev --build-optimizer --app universal --output-hashing=none",
    "build:dev-new": "ng build --prod --e=dev --experimental-angular-compiler&& ng build --prod --e=dev --app universal --output-hashing=none --experimental-angular-compiler",
    "build:prod": "ng build --prod --build-optimizer && ng build --prod --build-optimizer --app universal --output-hashing=none",
    "build:dynamic": "npm run build:prod && npm run webpack:server",
    "webpack:server": "webpack --display-error-details --config webpack.server.config.js --progress --colors",
    "serve:dynamic": "node dist/server.js",
    "build:universal": "npm run build:dev && npm run webpack:server",
    "build:universal-new": "npm run build:dev-new && npm run webpack:server",
    "test": "karma start ./karma.conf.js",
    "lint": "ng lint",
    "e2e": "protractor ./protractor.conf.js",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "release": "standard-version && git pushâ€Š--follow-tags origin master",
    "angular:latest": "bash -c 'npm i @angular/{animations,common,compiler,compiler-cli,core,forms,http,language-service,platform-browser,platform-browser-dynamic,platform-server,router}@latest'",
    "angular:next": "bash -c 'npm i @angular/{animations,common,compiler,compiler-cli,core,forms,http,language-service,platform-browser,platform-browser-dynamic,platform-server,router}@next'",
    "angular:builds": "bash -c 'npm i angular/{animations,common,compiler,compiler-cli,core,forms,http,language-service,platform-browser,platform-browser-dynamic,platform-server,router}-builds'",
    "angular:update": "bash -c 'npm up @angular/{animations,common,compiler,compiler-cli,core,forms,http,language-service,platform-browser,platform-browser-dynamic,platform-server,router}'",
    "cli:latest": "npm un @angular/cli && npm un @angular/cli -g && npm i @angular/cli@latest -g && npm i @angular/cli@latest -D",
    "cli:next": "npm un @angular/cli && npm un @angular/cli -g && npm i @angular/cli@next -g && npm i @angular/cli@next -D",
    "cli:builds": "npm un @angular/cli && npm un @angular/cli -g && npm i angular/cli-builds -g && npm i angular/cli-builds -D",
    "material:latest": "npm i @angular/{material,cdk,flex-layout}@latest",
    "material:builds": "npm i angular/{material,cdk,flex-layout}-builds",
    "material:update": "npm up @angular/{material,cdk,flex-layout}"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "github:angular/animations-builds",
    "@angular/app-shell": "^0.1.0",
    "@angular/cdk": "github:angular/cdk-builds",
    "@angular/common": "github:angular/common-builds",
    "@angular/compiler": "github:angular/compiler-builds",
    "@angular/core": "github:angular/core-builds",
    "@angular/flex-layout": "github:angular/flex-layout-builds",
    "@angular/forms": "github:angular/forms-builds",
    "@angular/http": "github:angular/http-builds",
    "@angular/material": "github:angular/material2-builds",
    "@angular/platform-browser": "github:angular/platform-browser-builds",
    "@angular/platform-browser-dynamic": "github:angular/platform-browser-dynamic-builds",
    "@angular/platform-server": "github:angular/platform-server-builds",
    "@angular/router": "github:angular/router-builds",
    "@nguniversal/common": "^5.0.0-beta.4",
    "@nguniversal/express-engine": "^5.0.0-beta.4",
    "@nguniversal/module-map-ngfactory-loader": "^5.0.0-beta.4",
    "@swimlane/ngx-charts": "^6.1.0",
    "apollo-angular": "^1.0.0-beta.1",
    "apollo-angular-link-http": "0.0.1-beta.0",
    "apollo-cache-inmemory": "^0.2.0-rc.4",
    "apollo-client": "^2.0.1",
    "apollo-link": "^0.8.0",
    "core-js": "^2.5.1",
    "d3": "^4.11.0",
    "express": "^4.16.2",
    "express-http-proxy": "^1.0.7",
    "fetch-intercept": "^2.2.3",
    "graphql": "^0.11.7",
    "graphql-tag": "^2.5.0",
    "hammerjs": "^2.0.8",
    "helmet": "^3.9.0",
    "ng2-file-upload": "^1.2.1",
    "preboot": "^5.1.7",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.5.2",
    "uuid": "^3.1.0",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "^1.5.0-rc.8",
    "@angular/compiler-cli": "github:angular/compiler-cli-builds",
    "@angular/language-service": "github:angular/language-service-builds",
    "@angular/service-worker": "^1.0.0-beta.16",
    "@angularclass/hmr": "^2.1.3",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^8.0.47",
    "@types/zen-observable": "^0.5.3",
    "codelyzer": "^3.2.2",
    "encoding": "^0.1.12",
    "html-loader": "^0.5.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "node-loader": "^0.6.0",
    "protractor": "~5.1.2",
    "shebang-loader": "0.0.1",
    "standard-version": "^4.2.0",
    "ts-loader": "^3.0.5",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "^2.4.2"
  }
}
