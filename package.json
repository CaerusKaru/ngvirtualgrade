{
  "name": "ngvirtualgrade",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "start:hmr": "ng serve --hmr --e=hmr --proxy-config proxy.conf.json",
    "build:dev": "ng build --prod --e=dev && ng build --prod --e=dev --app universal --output-hashing=none",
    "build:prod": "ng build --prod && ng build --prod --app universal --output-hashing=none",
    "build:ssr": "npm run build:prod && npm run webpack:server",
    "build:lssr": "npm run build:dev && npm run webpack:server",
    "webpack:server": "webpack --display-error-details --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server.js",
    "test": "karma start ./karma.conf.js",
    "lint": "ng lint",
    "e2e": "protractor ./protractor.conf.js",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "release": "standard-version && git pushâ€Š--follow-tags origin master",
    "angular:latest": "bash -c 'npm i @angular/{animations,common,compiler,compiler-cli,core,forms,http,language-service,platform-browser,platform-browser-dynamic,platform-server,router,service-worker}@latest'",
    "angular:next": "bash -c 'npm i @angular/{animations,common,compiler,compiler-cli,core,forms,http,language-service,platform-browser,platform-browser-dynamic,platform-server,router,service-worker}@next'",
    "angular:builds": "bash -c 'npm i angular/{animations,common,compiler,compiler-cli,core,forms,http,language-service,platform-browser,platform-browser-dynamic,platform-server,router,service-worker}-builds'",
    "angular:update": "bash -c 'npm up @angular/{animations,common,compiler,compiler-cli,core,forms,http,language-service,platform-browser,platform-browser-dynamic,platform-server,router,service-worker}'",
    "cli:latest": "npm un @angular/cli && npm un @angular/cli -g && npm i @angular/cli@latest -g && npm i @angular/cli@latest -D",
    "cli:next": "npm un @angular/cli && npm un @angular/cli -g && npm i @angular/cli@next -g && npm i @angular/cli@next -D",
    "cli:builds": "npm un @angular/cli && npm un @angular/cli -g && npm i angular/cli-builds -g && npm i angular/cli-builds -D",
    "material:latest": "bash -c 'npm i @angular/{material,cdk,flex-layout}@latest'",
    "material:builds": "bash -c 'npm i angular/{material,cdk,flex-layout}-builds'",
    "material:update": "bash -c 'npm up @angular/{material,cdk,flex-layout}'"
  },
  "private": true,
